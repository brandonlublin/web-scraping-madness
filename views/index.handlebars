<div id="wrapper">
    <h1>Note Scraper</h1>
    <h2><i>Latest News in NBA Free Agency</i></h2>
    <div id="col s3"></div>
    <h5 class='card z-depth-3 center-align col s6'>Click 'Scrape' to get the latest news Articles from Bleacher Report.</h5>
</div>
<div class="row">
    <div class='col s2'></div>
    <div class='col s8 left-align'>
        {{#each article}}
            <div>
                <h3>{{this.title}}</h3>
                <a href="/readArticle/{{this._id}}" class='waves-effect waves-light btn' id='signUp'>Read Article</a>
            </div>
        {{/each}}
    </div>
</div>